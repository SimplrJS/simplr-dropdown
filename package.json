  "files": [
    "dist",
    "**/*.md",
    "@types"
  ],
  "devDependencies": {
    "rollup": "^0.56.3",
    "rollup-plugin-typescript2": "^0.11.1",
    "rollup-plugin-uglify": "^3.0.0",
    "simplr-tslint": "^1.0.0-alpha.7",
    "ts-loader": "^4.0.0",
    "tslint": "^5.9.1",
    "tslint-language-service": "^0.9.8",
    "typescript": "^2.7.2",
    "uglify-js": "^3.3.12",
    "webpack": "^4.0.1"
  },
  "dependencies": {
