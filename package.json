{
  "name": "simplr-dropdown",
  "version": "3.1.1",
  "description": "A flexible React dropdown component.",
  "types": "@types/index.d.ts",
  "main": "dist/simplr-dropdown.js",
  "scripts": {
    "test": "tsc -p . --noEmit && npm run tslint",
