  "files": [
    "dist",
    "**/*.md",
    "@types"
  ],
  "devDependencies": {
    "ts-loader": "^3.1.1",
    "tslint": "^5.4.3",
    "typescript": "^2.3.4",
    "uglify-js": "^3.0.20",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "@types/prop-types": "^15.5.1",
